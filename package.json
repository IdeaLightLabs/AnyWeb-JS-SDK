{
  "name": "@idealight-labs/anyweb-js-sdk",
  "description": "AnyWeb JavaScript Software Development Kit",
  "version": "1.0.5",
  "license": "LGPL-3.0",
  "author": "common@idealight.ltd",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IdeaLightLabs/AnyWeb-JS-SDK.git"
  },
  "keywords": [
    "anyweb",
    "sdk"
  ],
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "browser": "dist/anyweb-js-sdk.umd.min.js",
  "browserify-browser": {
    "secp256k1": "secp256k1/elliptic"
  },
  "files": [
    "dist",
    "src",
    "lib",
    "types"
  ],
  "browserslit": "cover 99.5%",
  "dependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@conflux-dev/conflux-address-js": "^1.3.12",
    "abi-util-lite": "^0.1.0",
    "axios": "^0.25.0",
    "big.js": "^5.2.2",
    "commander": "^8.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "js-conflux-sdk": "^2.0.0",
    "keccak": "^2.0.0",
    "node-forge": "^1.2.1",
    "rlp": "^2.2.7",
    "scrypt-js": "^3.0.1",
    "secp256k1": "^3.7.1",
    "superagent": "^6.1.0",
    "typescript": "^4.5.5",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
